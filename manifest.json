{
    "manifest_version" : 2,
    "name" : "Curlai - Browser Extension",
    "description" : "Beta: Youtube Clutter Free ",
    "version" : "0.1",
    "content_scripts" : [
    {
        "matches" : [
            "https://www.youtube.com/*"
        ],
        "js" : [
            "env.js",
            "content.js"
        ]
    }]
}